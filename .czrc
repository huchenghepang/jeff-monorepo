{
  "path": "node_modules/cz-git",
  "useEmoji": true,
  "emojiAlign": "center",
  "useAI": false,
  "aiNumber": 1,
  "themeColorCode": "",
  "scopes": [
    "root",
    "backend",
    "frontend",
    "components",
    "utils",
    "scripts",
    "config"
  ],
  "scopeOverrides": {
    "fix": [
      {
        "value": "root",
        "name": "root:     根目录"
      },
      {
        "value": "backend",
        "name": "backend:  后端应用"
      },
      {
        "value": "frontend",
        "name": "frontend: 前端应用"
      },
      {
        "value": "components",
        "name": "components: 组件库"
      },
      {
        "value": "utils",
        "name": "utils:     工具库"
      },
      {
        "value": "scripts",
        "name": "scripts:   脚本"
      },
      {
        "value": "config",
        "name": "config:   配置文件"
      }
    ]
  },
  "allowCustomScopes": true,
  "allowEmptyScopes": true,
  "customScopesAlign": "bottom",
  "customScopesAlias": "custom:     自定义",
  "emptyScopesAlias": "empty:      不填写",
  "upperCaseSubject": false,
  "markBreakingChangeMode": false,
  "allowBreakingChanges": [
    "feat",
    "fix"
  ],
  "breaklineNumber": 100,
  "breaklineChar": "|",
  "skipQuestions": [
    "body"
  ],
  "issuePrefixes": [
    {
      "value": "closed",
      "name": "closed:   已关闭"
    },
    {
      "value": "fixed",
      "name": "fixed:    已修复"
    },
    {
      "value": "solved",
      "name": "solved:   已解决"
    }
  ],
  "customIssuePrefixAlign": "top",
  "emptyIssuePrefixAlias": "skip:      跳过",
  "customIssuePrefixAlias": "custom:   自定义",
  "allowCustomIssuePrefix": true,
  "allowEmptyIssuePrefix": true,
  "confirmColorize": true,
  "maxHeaderLength": 108,
  "maxSubjectLength": 100,
  "minSubjectLength": 0,
  "defaultScope": "",
  "defaultSubject": "",
  "defaultBody": "",
  "defaultIssues": "",
  "types": [
    {
      "value": "feat",
      "name": "feat:     ✨新增功能",
